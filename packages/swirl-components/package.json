{
  "name": "@getflip/swirl-components",
  "version": "0.1.0",
  "description": "Swirl Design System Web Component Library",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/swirl-components/swirl-components.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/getflip/swirl"
  },
  "files": [
    "dist/",
    "loader/",
    "vscode-data.json"
  ],
  "scripts": {
    "start": "yarn stencil:build && concurrently \"npm:*:start\"",
    "dev": "yarn start",
    "test": "yarn stencil:test",
    "build": "yarn stencil:build && yarn storybook:build",
    "generate": "plop",
    "lint": "eslint src/**/*{.ts,.tsx}",
    "stencil:build": "stencil build",
    "stencil:start": "stencil build --watch",
    "stencil:test": "stencil test --spec",
    "stencil:test:watch": "stencil test --spec --watchAll",
    "storybook:start": "start-storybook -p 6006 --no-manager-cache",
    "storybook:build": "build-storybook"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.0.2",
    "@getflip/swirl-icons": "^0.2.0",
    "@getflip/swirl-tokens": "^1.0.0",
    "@stencil/core": "^2.18.0",
    "a11y-dialog": "^7.5.2",
    "classnames": "^2.3.2",
    "color2k": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@pxtrn/storybook-addon-docs-stencil": "^6.4.1",
    "@stencil/angular-output-target": "^0.4.0",
    "@stencil/eslint-plugin": "^0.4.0",
    "@stencil/postcss": "^2.1.0",
    "@stencil/react-output-target": "^0.3.1",
    "@storybook/addon-a11y": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addons": "^6.5.12",
    "@storybook/builder-vite": "^0.2.2",
    "@storybook/html": "^6.5.12",
    "@storybook/theming": "^6.5.12",
    "@types/autoprefixer": "^10.2.0",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/jest": "^27.0.3",
    "@types/mdx": "^2.0.2",
    "@typescript-eslint/parser": "^5.38.0",
    "autoprefixer": "^10.4.12",
    "babel-loader": "^8.2.5",
    "body-scroll-lock": "^4.0.0-beta.0",
    "chromatic": "^6.10.0",
    "concurrently": "^7.4.0",
    "eslint-plugin-react": "^7.31.8",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "plop": "^3.1.1",
    "postcss-custom-media": "^8.0.2",
    "postcss-nested": "^5.0.6",
    "storybook-addon-themes": "^6.1.0",
    "storybook-design-token": "2.7.0",
    "svgo": "^2.8.0",
    "typescript": "^4.8.3",
    "vite": "^3.1.3"
  },
